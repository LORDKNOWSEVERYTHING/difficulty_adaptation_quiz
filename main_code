questions_attempted=[]
esy=[]
med=[]
hrd=[]
user_score_e=[]
user_score_m=[]
user_score_h=[]
for i in questions:
    if i[1]==1:
        esy.append([i[0]]+[i[2]])
    elif i[1]==2:
        med.append([i[0]]+[i[2]])
    else:
        hrd.append([i[0]]+[i[2]])
from tkinter import *
def easy():
    def submite():
        ans=easyv.get()
        questions_attempted[-1]+=[ans]
        if ans==que[1]: 
            user_score_e.append(1)
            n.append(1)
        else:
            user_score_e.append(0)
        win.destroy()
    n=[0]
    win=Tk()
    win.title('QUIZ')
    win.geometry('500x560')
    que=esy[0]
    questions_attempted.append(esy.pop(0))
    Label(win,text=que[0]).pack(pady=10)
    easyv=StringVar()
    Entry(win,textvariable=easyv).pack(pady=10)
    Button(win,text='SUBMIT',command=submite).pack(pady=10)
    win.mainloop()
    return n[-1]
def medium():
    def submitm():
        ans=mediumv.get()
        questions_attempted[-1]+=[ans]
        if ans==que[1]: 
            user_score_m.append(1)
            n.append(1)
        else:
            user_score_m.append(0)
        win.destroy()
    n=[0]
    win=Tk()
    win.title('QUIZ')
    win.geometry('500x560')
    que=med[0]
    questions_attempted.append(med.pop(0))
    Label(win,text=que[0]).pack(pady=10)
    mediumv=StringVar()
    Entry(win,textvariable=mediumv).pack(pady=10)
    Button(win,text='SUBMIT',command=submitm).pack(pady=10)
    win.mainloop()
    return n[-1]
def hard():
    def submith():
        ans=hardv.get()
        questions_attempted[-1]+=[ans]
        if ans==que[1]:
            user_score_h.append(1)
            n.append(1)
        else:
            user_score_h.append(0)
        win.destroy()
    n=[0]
    win=Tk()
    win.title('QUIZ')
    win.geometry('500x560')
    que=hrd[0]
    questions_attempted.append(hrd.pop(0))
    Label(win,text=que[0]).pack(pady=10)
    hardv=StringVar()
    Entry(win,textvariable=hardv).pack(pady=10)
    Button(win,text='SUBMIT',command=submith).pack(pady=10)
    win.mainloop()
    return n[-1]
