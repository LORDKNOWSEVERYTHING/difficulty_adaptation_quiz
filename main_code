    Label(win2,text='HARD CORRECT : '+str(hrdc)).pack(pady=10)
    Label(win2,text='HARD INCORRECT : '+str(hrdi)).pack(pady=10)
    score=(1*esyc)+(2*medc)+(3*hrdc)
    Label(win2,text='TOTAL SCORE : '+str(score)).pack(pady=10)
    Button(win2,text='OK',command=win2.destroy).pack(pady=10)
    Button(win2,text='PAPER ANALYSIS',command=analyse).pack(pady=10)
    win2.mainloop()
