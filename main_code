questions=[('EASY1',1,'Y'),('MEDIUM1',2,'Y'),('HARD1',3,'Y'),
           ('EASY2',1,'Y'),('MEDIUM2',2,'Y'),('HARD2',3,'Y'),
           ('EASY3',1,'Y'),('MEDIUM3',2,'Y'),('HARD3',3,'Y'),
           ('EASY4',1,'Y'),('MEDIUM4',2,'Y'),('HARD4',3,'Y'),
           ('EASY5',1,'Y'),('MEDIUM5',2,'Y'),('HARD5',3,'Y'),
