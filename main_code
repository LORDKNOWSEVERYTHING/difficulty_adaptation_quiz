    win2=Tk()
    win2.title('RESULT')
    win2.geometry('500x560')
    Label(win2,text='THANKYOU FOR TAKING THIS QUIZ.\n YOUR RESULTS ARE AS FOLLOWS : ').pack(pady=10)
    esyatt=0
    medatt=0
    hrdatt=0
    esyc=0
    medc=0
    hrdc=0
    esyi=0
    medi=0
    hrdi=0
    for i in user_score_e:
        esyatt+=1
        if i:
            esyc+=1
        else:
            esyi+=1
    for i in user_score_m:
        medatt+=1
        if i:
            medc+=1
        else:
            medi+=1
    for i in user_score_h:
        hrdatt+=1
        if i:
            hrdc+=1
        else:
            hrdi+=1
    def analyse():
        win2.destroy()
        toquit=[1]
        j=0
        while j<len(questions_attempted) and toquit[-1]:
            i=questions_attempted[j]
            win=Tk()
            win.title('PAPER ANALYSIS')
            toquitl=[0]
            win.geometry('500x560')
            num=j+1
            Label(win,text='QUESTION '+str(num)+'.').pack(pady=10)
            Label(win,text='QUESTION : ').pack(pady=10)
            Label(win,text=i[0]).pack(pady=10)
            Label(win,text='CORRECT ANSWER : ').pack(pady=10)
            Label(win,text=i[1]).pack(pady=10)
            Label(win,text='YOUR ANSWER : ').pack(pady=10)
            Label(win,text=i[2]).pack(pady=10)
            Button(win,text='NEXT',command=win.destroy).pack(pady=10)
            def toquitf():
                win.destroy()
                toquit.append(0)
            Button(win,text='QUIT',command=toquitf).pack(pady=10)
            win.mainloop()
            j+=1
    Label(win2,text='EASY ATTEMPTED : '+str(esyatt)).pack(pady=10)
    Label(win2,text='EASY CORRECT : '+str(esyc)).pack(pady=10)
    Label(win2,text='EASY INCORRECT : '+str(esyi)).pack(pady=10)
    Label(win2,text='MEDIUM ATTEMPTED : '+str(medatt)).pack(pady=10)
