score=[]
from tkinter import *
def easy():
    def submite():
        ans=easyv.get()
        if ans=='Y': 
            score.append(1)
            n.append(1)
        win.destroy()
    n=[0]
    win=Tk()
    win.geometry('500x500')
    Label(win,text='EASY : ').pack(pady=10)
    easyv=StringVar()
    Entry(win,textvariable=easyv).pack(pady=10)
    Button(win,text='SUBMIT',command=submite).pack(pady=10)
    win.mainloop()
    return n[-1]
def medium():
    def submitm():
        ans=mediumv.get()
        if ans=='Y': 
            score.append(1)
            n.append(1)
        win.destroy()
    n=[0]
    win=Tk()
    win.geometry('500x500')
    Label(win,text='MEDIUM : ').pack(pady=10)
    mediumv=StringVar()
    Entry(win,textvariable=mediumv).pack(pady=10)
    Button(win,text='SUBMIT',command=submitm).pack(pady=10)
    win.mainloop()
    return n[-1]
def hard():
    def submith():
        ans=hardv.get()
        if ans=='Y':
            score.append(1)
            n.append(1)
        win.destroy()
    n=[0]
    win=Tk()
    win.geometry('500x500')
    Label(win,text='HARD : ').pack(pady=10)
    hardv=StringVar()
    Entry(win,textvariable=hardv).pack(pady=10)
    Button(win,text='SUBMIT',command=submith).pack(pady=10)
    win.mainloop()
    return n[-1]
def begin():
    check.append(1)
    win.destroy()
check=[0]
win=Tk()
win.geometry('500x500')
Label(win,text='QUIZ CANNOT BE EXITED ONCE STARTED.').pack(pady=10)
Label(win,text='YOU CAN EXIT NOW IF YOU WANT TO.').pack(pady=10)
Button(win,text='BEGIN',command=begin).pack(pady=10)
Button(win,text='QUIT',command=win.destroy).pack(pady=10)
win.mainloop()
